{
  "name": "freedom-seed",
  "private": true,
  "type": "module",
  "prettier": "@epic-web/config/prettier",
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "build": "react-router build",
    "cf-typegen": "wrangler types",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply freedom-seed --local",
    "db:migrate:remote": "wrangler d1 migrations apply freedom-seed --remote",
    "db:seed": "node --no-warnings=ExperimentalWarning --experimental-strip-types scripts/db-seed.ts",
    "deploy": "npm run build && wrangler deploy",
    "dev": "react-router dev",
    "format": "prettier --write .",
    "icons:add": "node --no-warnings=ExperimentalWarning --experimental-strip-types scripts/icons.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "postinstall": "npm run cf-typegen",
    "preview": "npm run build && vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "typecheck": "npm run cf-typegen && react-router typegen && tsc -b",
    "validate": "npm run lint:fix && npm run typecheck && npm run format"
  },
  "dependencies": {
    "@conform-to/react": "^1.17.1",
    "@conform-to/zod": "^1.17.1",
    "@date-fns/tz": "^1.4.1",
    "@epic-web/client-hints": "^1.3.8",
    "@epic-web/config": "^1.24.0",
    "@mdx-js/rollup": "^3.1.1",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@react-email/components": "^1.0.8",
    "@react-email/render": "^2.0.4",
    "@sentry/cloudflare": "^10.40.0",
    "@simplewebauthn/browser": "^13.2.2",
    "@simplewebauthn/server": "^13.2.3",
    "arctic": "^3.7.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.45.1",
    "isbot": "^5.1.34",
    "otpauth": "^9.5.0",
    "qrcode": "^1.5.4",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router": "^7.9.4",
    "resend": "^6.9.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.5.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.1",
    "@cloudflare/vite-plugin": "^1.25.6",
    "@playwright/test": "^1.58.2",
    "@react-router/dev": "^7.9.4",
    "@tailwindcss/vite": "^4.2.1",
    "@types/mdx": "^2.0.13",
    "@types/node": "^25",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "drizzle-kit": "^0.31.9",
    "eslint": "^10.0.1",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "react-router-auto-routes": "^0.8.2",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-icons-spritesheet": "^3.0.1",
    "vite-tsconfig-paths": "^6.1.0",
    "vitest": "^4.0.18",
    "wrangler": "^4.45.0"
  }
}
